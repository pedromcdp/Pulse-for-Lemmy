{
  "expo": {
    "name": "Pulse",
    "slug": "pulse",
    "scheme": "pulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#04858F",
      "dark": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#323439"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.pedromcdp.pulse",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#04858F",
        "dark": {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#323439"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#04858F"
      },
      "userInterfaceStyle": "automatic",
      "package": "com.pedromcdp.pulse"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": true,
            "ios-deployment-target": "14.0"
          }
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "dff75167-02c8-43c5-ab19-45e85555834d"
      }
    },
    "owner": "pedromcdp"
  }
}
